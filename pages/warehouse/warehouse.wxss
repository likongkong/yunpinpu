page{
  background: #f5f5f5;
}
.head{
  height: 280rpx;
  background-color: #ffffff;
  padding-top: 80rpx;
  box-sizing: border-box;
}
.head text{
  text-align: center;
  display: block;
}
.head .myTicketNum{
  font-size: 73rpx;
  color: #e60026;
  margin-bottom: 12rpx;
}
.head .h3{
  font-size: 30rpx;
  color: #5e5e5e;
}

.myTicket{
  width: 100%;
  padding: 50rpx 70rpx 80rpx 70rpx;
  box-sizing: border-box;
}
.myTicket .h2{
  font-size: 30rpx;
  color: #3c3c3c;
  margin-bottom: 32rpx;
}
.ticket{
  width: 610rpx;
  height: 190rpx;
  margin-top: 30rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size:100% 100%; 
  /* padding: 17rpx 14rpx; */
  /* box-sizing: border-box; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.ticket>view{
  width: 574rpx;
  height: 154rpx;
  border: 2rpx solid #fff;
  display: flex;
  justify-content: space-between;
}
.ticket .people {
  padding-left: 24rpx;
  padding-top: 36rpx;
  box-sizing: border-box;
}
.ticket .people text{
  display: block;
}
.ticket .people .name{
  font-size: 34rpx;
  color: #ffffff;
  margin-bottom: 28rpx;
}
.ticket .people .time{
  font-size: 24rpx;
  color: #ffffff;
  opacity: .6
}
.ticket .num{
  width: 110rpx;
  border-left: 2rpx solid #fff; 
  line-height: 154rpx;
  text-align: center;
  font-size: 40rpx;
  color: #fff;
  font-weight: bold;
}
.sendTicket{
  width: 522rpx;
	height: 71rpx;
  font-size: 36rpx;
  color: #ffffff;
  line-height: 71rpx;
  background: #e60026;
}