page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
}

.logo{
  width: 188rpx;
  height: auto;
  margin: 68rpx auto 0 auto;
  display: block;
}

.content{
  width: 628rpx;
  height: 802rpx;
  /* background:#fff; */
  margin: 32rpx auto 0 auto;
  border-radius: 8rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100%;
}
.content{
  position: relative;
}
.content_t text{
  display: block;
  text-align: center;
}
.content_t{
  padding-top: 52rpx;
  box-sizing: border-box;
  text-align: center;
}
.content_t .text1{
  font-size: 32rpx;
  color: #ff4f50;
  margin-bottom: 30rpx;
}
.content_t .text2{
  font-size: 54rpx;
  color: #ff4f50;
}
.content_t .btn{
  display: inline-block;
  margin: 16rpx auto 30rpx auto; 
}
.content_t .btn text{
  display: inline-block;
  padding: 0 30rpx;
  box-sizing: border-box;
  height: 80rpx;
	background-color: #e60026;
	border-radius: 8rpx;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 80rpx;
}
.content_t .btn text:first-child{
  margin-right: 92rpx;
}
.content_t .introduce{
  display: block;
  text-align: center;
  color: #3e3e3e;
  font-size: 32rpx;
}

.content_b{
  position: absolute;
  bottom: 50rpx;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: center;
}
.content_b image{
  width: 312rpx;
  height: auto;
}