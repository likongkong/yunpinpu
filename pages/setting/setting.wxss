page{
  background: #f5f5f5;
}
.box{
  width: 100%;
  height: 98rpx;
  border-bottom: 2rpx solid #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.box text{
  font-size: 28rpx;
  color: #111111;
}
.box image{
  width: 24rpx;
  height: auto;
}
.head{
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.head>view:last-child{
  border: none;
}

.head>view{
  border-bottom: 2rpx solid #f5f5f5;
}

.single{
  width: 100%;
  height: 98rpx;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 0 30rpx;
  background: #fff;
  box-sizing: border-box;
  margin-top: 24rpx;
}

.signOut{
  width: 610rpx;
	height: 88rpx;
	background-color: #e60026;
	border-radius: 8rpx;
  position: fixed;
  bottom: 58rpx;
  left: 50%;
  margin-left: -305rpx;
  font-size: 36rpx;
  color: #ffffff;
  line-height: 88rpx;
}
.signOut::after{
  border: none;
}
