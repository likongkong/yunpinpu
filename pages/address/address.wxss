page{
  background: #f5f5f5;
}
.head{
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
}
.head text{
  display: block;
  width: 130rpx;
  height: 100rpx;
  position: absolute;
  top: 0;right: 0;
  color: #000000;
  font-size: 30rpx;
  text-align: center;
  line-height: 100rpx;
}

.content{
  margin-top: 100rpx;
  background: #fff;
}
.content .content-box{
  width: 690rpx;
  margin-left: 30rpx;
}
.content .content-m{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .content-m>view{
  font-size: 28rpx;
}
.content .consignee{
  border-bottom: 2rpx solid #f5f5f5;
}
.content .content-m>view:nth-child(1){
  width:190rpx;
  color: #111111;
}
.content .content-m>view:nth-child(2){
  width:433rpx;
  color: #c6c6c6;
}
.content .content-m>view:nth-child(3){
  width:65rpx;
}
.content .content-m>view:nth-child(3) image{
  width: 26rpx;
  height: 28rpx;
  float: right;
}

.delete{
  width: 580rpx;
  height: 88rpx;
  background: #e60026;
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 36rpx;
  color: #fff;
  margin-top: 52rpx;
}
.delete::after{
  border: none;
}